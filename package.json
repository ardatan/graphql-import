{
  "name": "graphql-import",
  "version": "1.0.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ardatan/graphql-import"
  },
  "homepage": "https://github.com/ardatan/graphql-import#readme",
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "bob",
    "test": "jest --no-watchman --passWithNoTests",
    "lint": "tslint src/**/*.ts",
    "ci:release:canary": "node bump.js && bob-update-version && npm publish dist --tag alpha"
  },
  "peerDependencies": {
    "graphql": "^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0"
  },
  "devDependencies": {
    "bob-the-bundler": "0.3.4",
    "@types/jest": "25.2.1",
    "jest": "25.5.0",
    "ts-jest": "25.4.0",
    "@types/node": "13.13.4",
    "graphql": "15.0.0",
    "tslint": "6.1.2",
    "tslint-config-standard": "9.0.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@graphql-toolkit/core": "0.10.5",
    "@graphql-toolkit/code-file-loader": "0.10.5",
    "@graphql-toolkit/url-loader": "0.10.5",
    "@graphql-toolkit/json-file-loader": "0.10.5",
    "@graphql-toolkit/graphql-file-loader": "0.10.5",
    "@graphql-toolkit/schema-merging": "0.10.5"
  }
}
